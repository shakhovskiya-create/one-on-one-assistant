# Backend Configuration
BACKEND_WS_URL=wss://one-on-one-back-production.up.railway.app/ws/connector
CONNECTOR_API_KEY=your-connector-api-key-here

# Active Directory / LDAP Configuration
AD_URL=ldap://your-ldap-server:389
AD_BASE_DN=OU=USERS,DC=example,DC=com
AD_BIND_USER=domain\service-account
AD_BIND_PASSWORD=service-account-password
AD_SKIP_VERIFY=true

# Exchange Web Services
EWS_URL=https://post.ekf.su/EWS/Exchange.asmx
EWS_DOMAIN=ekfgroup
EWS_USERNAME=domain\service-account
EWS_PASSWORD=service-account-password
EWS_SKIP_TLS_VERIFY=true
