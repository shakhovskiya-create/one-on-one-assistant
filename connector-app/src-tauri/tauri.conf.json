{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "EKF Connector",
  "version": "1.0.0",
  "identifier": "ru.ekf.connector",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "EKF Connector",
        "width": 480,
        "height": 640,
        "resizable": true,
        "center": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg"],
    "icon": [],
    "macOS": {
      "minimumSystemVersion": "10.15"
    }
  }
}
